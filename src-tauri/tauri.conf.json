{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Hello World Explorer",
  "version": "0.1.0",
  "identifier": "dev.helloworld.explorer",
  "build": {
    "frontendDist": "../src"
  },
  "app": {
    "windows": [
      {
        "title": "Hello World Explorer",
        "width": 1280,
        "height": 820,
        "minWidth": 900,
        "minHeight": 600,
        "resizable": true,
        "fullscreen": false,
        "decorations": true
      }
    ],
    "withGlobalTauri": true,
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
    "icons/128x128.png",
    "icons/Square310x310Logo.png",
    "icons/32x32.png",
    "icons/Square150x150Logo.png"
    ],
    "resources": {
      "binaries/*": "binaries/"
    }
  }
}
